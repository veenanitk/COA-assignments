#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x19ded10 .scope module, "test" "test" 2 8;
 .timescale -9 -12;
v0x19ecb60_0 .var "clk", 0 0;
S_0x19bb770 .scope module, "f" "hello" 2 10, 3 5, S_0x19ded10;
 .timescale -9 -12;
v0x19bb860_0 .net "clk", 0 0, v0x19ecb60_0; 1 drivers
E_0x19dec10 .event edge, v0x19bb860_0;
    .scope S_0x19bb770;
T_0 ;
    %wait E_0x19dec10;
    %vpi_call 3 9 "$monitor", "%0d Hello World\012", $time;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x19ded10;
T_1 ;
    %set/v v0x19ecb60_0, 1, 1;
    %movi 8, 99, 8;
T_1.0 %cmp/s 0, 8, 8;
    %jmp/0xz T_1.1, 5;
    %add 8, 1, 8;
    %delay 1000, 0;
    %load/v 16, v0x19ecb60_0, 1;
    %inv 16, 1;
    %set/v v0x19ecb60_0, 16, 1;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "t_hello.v";
    "hello.v";
